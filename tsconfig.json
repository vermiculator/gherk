{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "/dist",
    "/src/content/.obsidian",
    "/src/content/.trash",
    "/src/content/.Rubbish",
    "/src/content/private",
    "/src/content/seeds",
    "/src/content/themes"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}