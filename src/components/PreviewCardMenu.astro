---
interface Props {
	link?: string;
}

const { link } = Astro.props;

import {
  Command,
  CommandEmpty,
  CommandGroup,
  CommandInput,
  CommandItem,
  CommandList,
  CommandSeparator,
} from "@/components/ui/command"

import {
  ArrowRightIcon,
  ArrowLeftIcon
} from "lucide-react"

import { Button } from "@/components/ui/button"
import { ButtonGroup } from "@/components/ui/button-group"
---


      <ButtonGroup className="hidden sm:flex">
        <Button variant="outline" size="icon" aria-label="See More">
          <ArrowRightIcon />
          <a href={link}>See More</a>
        </Button>
    <Command className="rounded-lg border shadow-md md:min-w-[450px]">
      <CommandInput placeholder="Type to search..." />
      <CommandList>
        <CommandEmpty>hmmm...</CommandEmpty>
        <CommandGroup heading="External">
          <CommandItem>
          <ArrowLeftIcon />
            <span>Wikipedia</span>
          </CommandItem>
          <CommandItem disabled>
          <ArrowLeftIcon />
            <span>Google</span>
          </CommandItem>
          <CommandItem disabled>
            <ArrowLeftIcon />
            <span>Quant</span>
          </CommandItem>
        </CommandGroup>
        <CommandSeparator />
        <CommandGroup heading="What!?">
          <CommandItem>
            <ArrowLeftIcon />
            <span>Explain</span>
          </CommandItem>
          <CommandItem>
            <ArrowLeftIcon />
            <span>Take Me Back Home</span>
          </CommandItem>
        </CommandGroup>
      </CommandList>
    </Command>
    </ButtonGroup>