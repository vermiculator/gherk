---

import Layout from '../components/Layout.astro';
import { Card, LinkCard, CardGrid } from '@astrojs/starlight/components';
import { getCollection } from 'astro:content';
const allItems = await getCollection('earth');

---

<Layout pageTitle='earth' pageStyle='leftside' starlightSettings={{next: false, prev: false}}>

<CardGrid>
    {  allItems.map((item:any) =>
      <LinkCard title={item.data.title ?? 'title'} href={'/earth/' + item.id} />
      )
    }
 </CardGrid>
</Layout>