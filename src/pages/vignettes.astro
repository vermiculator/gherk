---
import Layout from '../components/Layout.astro';
import { Card, CardGrid } from '@astrojs/starlight/components';
const allItems = Object.values(await import.meta.glob('./vignettes/data/*.md', { eager: true }));
---

<Layout pageTitle='vignettes' pageStyle='leftside' includes_p5 starlightSettings={{prev: false, next: false}}>
 <CardGrid>
    {  allItems.map((item:any) =>
      <Card title="">
          {item.frontmatter.url}
      </Card>
      )
    }
  </CardGrid>
</Layout>